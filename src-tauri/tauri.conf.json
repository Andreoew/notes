{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist"
  },
  "package": {
    "productName": "notes",
    "version": "../package.json"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      },
      "fs": {
        "all": false,
        "copyFile": false,
        "createDir": true,
        "exists": false,
        "readDir": true,
        "readFile": false,
        "removeDir": false,
        "removeFile": false,
        "renameFile": false,
        "scope": ["$APP/*", "$DOCUMENT/*", "$DOWNLOAD/*"],
        "writeFile": true
      },
      "dialog": {
        "all": false,
        "ask": false,
        "confirm": false,
        "message": true,
        "open": false,
        "save": false
      },
      "notification": {
        "all": true
      },
      "os": {
        "all": true
      },
      "path": {
        "all": true
      }
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "Experts Notes",
        "width": 800,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    },
    "updater": {
      "active": true,
      "dialog": true,
      "endpoints": [
        "http://[::1]:5001/tauri-releases/notes/{{target}}/{{current_version}}"
      ],
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IEJCNkM1Q0FDMjY2ODI5RDMKUldUVEtXZ21yRnhzdTZqeW5aaWltdHpMNEN2MzEvb3JPM0x2RzBmY3pGVlVlK0s1QWpmVEFTN2QK"
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "br.com.adss.expertnotes",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    }
  }
}
